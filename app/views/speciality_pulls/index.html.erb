<p id="notice"><%= notice %></p>

<h1>Speciality Pulls</h1>

<table>
  <thead>
    <tr>
      <th>Key</th>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @speciality_pulls.each do |speciality_pull| %>
      <tr>
        <td><%= speciality_pull.key %></td>
        <td><%= speciality_pull.name %></td>
        <td><%= link_to "Оценочный лист", speciality_pull.ocen_list.url(:original, false) %></td>
        <td><%= link_to 'Show', speciality_pull %></td>
        <td><%= link_to 'Edit', edit_speciality_pull_path(speciality_pull) %></td>
        <td><%= link_to 'Destroy', speciality_pull, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Speciality Pull', new_speciality_pull_path %>
