<p id="notice"><%= notice %></p>

<h1>University Pulls</h1>

<table>
  <thead>
    <tr>
      <th>Key</th>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @university_pulls.each do |university_pull| %>
      <tr>
        <td><%= university_pull.key %></td>
        <td><%= university_pull.name %></td>
        <td><%= link_to 'Show', university_pull %></td>
        <td><%= link_to 'Edit', edit_university_pull_path(university_pull) %></td>
        <td><%= link_to 'Destroy', university_pull, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New University Pull', new_university_pull_path %>
